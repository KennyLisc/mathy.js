#!/usr/bin/env node

var Engine = require('../index.js').Engine;

var engine = new Engine({
    name: 'a',
    derivation: process.argv.slice(2)
        .reduce(function (x, y) {
            return x + ' ' + y;
        }, ''
    )
});

var result = engine.process();

process.stdout.write(result[0] + '\n');